{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [


    {
      "type": "node",
      "request": "attach",
      "name": "Node: Debugger",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    },
    
    {
      "type": "node",
      "request": "launch",
      "name": "Node:Day Before Confirmation Emails",
      "program": "${workspaceFolder}/src/confirmations/emails.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Day of Confirmation Text",
      "program": "${workspaceFolder}/src/confirmations/sms.js",
      "restart": true
    },
    
    {
      "type": "node",
      "request": "launch",
      "name": "timeslot list",
      "program": "${workspaceFolder}/packages/wtc/timeslots/list.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Dates",
      "program": "${workspaceFolder}/packages/wtc/dates/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Times",
      "program": "${workspaceFolder}/packages/wtc/times/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Hold",
      "program": "${workspaceFolder}/packages/wtc/hold/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Reserve",
      "program": "${workspaceFolder}/packages/wtc/reserve/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Delete Stale Holds",
      "program": "${workspaceFolder}/packages/wtc/delete_holds/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cancel Hold",
      "program": "${workspaceFolder}/packages/wtc/cancel/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Waitlist",
      "program": "${workspaceFolder}/packages/wtc/waitlist/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Status",
      "program": "${workspaceFolder}/packages/wtc/status/run.js",
      "restart": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Send SMS",
      "program": "${workspaceFolder}/packages/wtc/sms/run.js",
      "restart": true
    },  
    {
      "type": "node",
      "request": "launch",
      "name": "Send Email",
      "program": "${workspaceFolder}/packages/wtc/sendgrid/run.js",
      "restart": true
    },
  ]
}
